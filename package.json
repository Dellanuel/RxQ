{
  "name": "rxq",
  "version": "0.7.0-beta.9",
  "description": "A reactive wrapper for the Qlik Analytics Platform APIs",
  "main": "build/rxq.js",
  "qix-version": "12.34.11",
  "scripts": {
    "build-qix-schemas": "node scripts/build-qix-schemas.js",
    "build-operators": "node scripts/build-operators.js",
    "update-operators": "npm run build-qix-schemas && npm run build-operators",
    "compile-cjs": "node scripts/compile_cjs.js",
    "compile-esm5": "node scripts/compile_esm5.js",
    "make-packages": "node scripts/make-packages.js",
    "build-dist-folder": "npm run compile-cjs && npm run compile-esm5 && npm run make-packages && npm run build && npm run build-min",
    "build": "webpack --progress --colors",
    "build-min": "webpack --progress --colors --env.minimize true",
    "build-release": "npm run build-dist-folder && cp -R dist/build/ rxq-build && zip rxq-build.zip rxq-build/* && rm -rf rxq-build/",
    "watch": "webpack --progress --colors --watch",
    "test": "mocha test/unit/*.spec.js",
    "test-e2e": "mocha test/e2e/*.spec.js",
    "test-compile": "mocha --compilers js:babel-core/register test/unit/*-spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axisgroup/RxQ.git"
  },
  "keywords": [
    "qap",
    "qix",
    "engine",
    "engineAPI",
    "qrs"
  ],
  "author": "Axis Group",
  "license": "ISC",
  "homepage": "https://github.com/axisgroup/RxQ#readme",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.26.0",
    "chai-generator": "^2.1.0",
    "dockerode": "^2.5.3",
    "enigma.js": "^2.1.1",
    "fs-extra": "^4.0.2",
    "mocha": "^4.0.1",
    "mock-socket": "^7.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "https-browserify": "0.0.1",
    "stream-http": "~2.0.1",
    "ws": "^1.1.1"
  },
  "peerDependencies": {
    "rxjs": "^5.5.0-beta.5"
  },
  "bugs": {
    "url": "https://github.com/axisgroup/RxQ/issues"
  },
  "browser": {
    "ws": false
  },
  "directories": {
    "example": "examples"
  }
}
